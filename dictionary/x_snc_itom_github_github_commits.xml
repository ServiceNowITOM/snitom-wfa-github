<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="GitHub Commits" max_length="40" name="x_snc_itom_github_github_commits" type="collection">
        <element label="Branch" max_length="32" name="branch" reference="x_snc_itom_github_github_branches" type="reference"/>
        <element label="Change_Additions" max_length="40" name="change_additions" type="integer"/>
        <element label="Change_Deletions" max_length="40" name="change_deletions" type="integer"/>
        <element label="Commiter" max_length="50" name="commiter" type="string"/>
        <element label="Commiter_Email" max_length="50" name="commiter_email" type="string"/>
        <element label="Commiter User" max_length="32" name="commiter_user" reference="sys_user" type="reference"/>
        <element label="Commit_Date" max_length="40" name="commit_date" type="glide_date_time"/>
        <element label="Message" max_length="2048" name="message" type="string"/>
        <element label="Repository" max_length="32" name="repository" reference="x_snc_itom_github_github_repositories" type="reference"/>
        <element display="true" label="SHA" max_length="41" name="sha" type="string"/>
        <element label="Total_Changes" max_length="40" name="total_changes" type="integer"/>
        <element label="Url" max_length="1024" name="url" type="url"/>
        <index name="index">
            <element name="commiter_user"/>
        </index>
        <index name="index2">
            <element name="repository"/>
        </index>
        <index name="index3">
            <element name="branch"/>
        </index>
    </element>
</database>
